      "action_required": "urgent_intervention"
    },
    "warning": {
      "condition": "< 80% of target",
      "notification": "daily",
      "escalation": "program_manager",
      "action_required": "action_plan"
    },
    "watch": {
      "condition": "< 90% of target",
      "notification": "weekly",
      "escalation": "team_lead",
      "action_required": "monitoring"
    },
    "achievement": {
